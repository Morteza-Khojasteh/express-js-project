<h1 class="text-center mb-3">{{title}}</h1>
<from action="/api/members/" method="POST" class="mb-4" form="form1">
  <div class="form-group">
    <label for="name">Name</label>
    <input type="text" name="name" class="form-control" />
  </div>
  <div class="form-group">
    <label for="email">Email</label>
    <input type="email" name="email" class="form-control" />
  </div></br>

</from>
  <button type="submit" form="form1" value="Submit" class="btn btn-primary btn-block mb-5">Add User</button>


<h4 class="text mb-1">Users</h4>
<ul class="list-group">
  {{#each members}}
    <li class="list-group-item">{{this.name}} : {{this.email}}</li>
  {{/each}}
</ul>

<a href="/api/members" class="btn-light mt-4">Visit API</a>